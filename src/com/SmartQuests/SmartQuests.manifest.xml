<manifest>
    <main-class>com.SmartQuests.SmartQuests</main-class>
    <name>Smart Quests</name>
    <tag-line>A bunch of quests at the click of a button</tag-line>
    <description>Currently only does tutorial island, but more to come</description>
    <version>0.0.1</version>
    <internal-id>SmartQuests</internal-id>
    <open-source>false</open-source>
    <hidden>false</hidden>
    <access>public</access>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>QUESTING</category>
    </categories>
    <tags>
        <tag>Tutorial Island</tag>
    </tags>
    <resources>
        <resource>com/SmartQuests/userInterface/SmartQuestsUIView.fxml</resource>
    </resources>
</manifest>